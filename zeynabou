tableau des commandes
Enregistrement 1 : (custumerid= 1, productid= 2, quantité= 3, order_date= '2023-01-22')
Enregistrement 2 : (custumerid= 2, productid= 1, quantité= 10, order_date= '2023-04-14')
INSRT INTO `commande`(order_id ,customer_id,product_id, quantité, order_date)
values('1','1`,`2`,`3`,`2023-01-22`);
values(`2`,`1`,`10` `2023-04-14`);
update `commandes` set `order_id =`[1]` , customer_id=`[1]` , product_id = `[2]`, quantité= [3],order_date[2023-01-22]  where client_id=11
update `commandes` set `order_id =`[2]` , customer_id=`[2]` , product_id = `[1]`, quantité= [10],order_date[2023-04-14]  where client_id=11
delete from `commandes` where 11
