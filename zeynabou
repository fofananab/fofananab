Enregistrement 1 : (nom= "Cookies", prix= 10)
Enregistrement 2 : (nom= "Candy", prix= 5,2)
INSERT INTO `produits`(` product_id`, `nom`, `prix`)
values('1', `cookies`,`10`);
values(`2`, `candy`, `5,2`);
update `produits` set `produits_id =`[1]` , nom=`[cookies]` , prix = `[10]` where product_id = 11;
update `produits` set `produits_id =`[2]` , nom=`[candy]` , prix = `[5,2]` where product_id_id = 11;
delete from `prouids` where 11;
